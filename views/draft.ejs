  <section id="footer">
    <div id="top">
      <div id="topLeft" class="<%= currentTeam.mascot.replace(/\s/g,'').toLowerCase() %>">
        <img src="logos/<%= currentTeam.mascot.replace(/\s/g,'').toLowerCase() %>.png" id="pickLogo" />
        <div id="pickTont">
          <div>
            <div class="pickCell">RD</div>
            <div class="pickCell">PK</div>
          </div>
          <div>
            <div class="pickCell"><%= round %></div>
            <div class="pickCell"><%= pick %></div>
          </div>
          <div id="pickClock" data-time="<%= time %>">0:<% if (time != 'x') { %><%= time %><% } else { %>30<% } %></div>
        </div>
      </div>
      <% if (!live) { %>
        <div id="pickMain" class="league">THE DRAFT IS NOT LIVE</div>
      <% } else { %>
        <div id="pickMain" class="<%= currentTeam.mascot.replace(/\s/g,'').toLowerCase() %>">THE <%= currentTeam.mascot.toUpperCase() %> ARE ON THE CLOCK</div>
      <% } %>
    </div>
    <div id="bot">
      <div id="left" class="league">
        <div id="next">Next</div>
        <svg style="height:16px;width:16px;margin:12px 0px;" viewBox="0 0 100 100" xmlns="http://www.w3.org/2000/svg">
          <polygon fill="currentColor" points="0 0, 0 100, 87 50"/>
        </svg>
      </div>
      <div id="right">
        <% nextTwenty.forEach(function(obj) { %>
          <% if (obj.n != -1) { %>
            <span class="<%= obj.c %>" data-pick="<%= obj.p %>"><%= obj.p %>. <%= obj.t %></span>
          <% } else { %>
            <span class="league" data-pick="-1">-----</span>
          <% } %>
        <% }); %>
      </div>
    </div>
  </section>
